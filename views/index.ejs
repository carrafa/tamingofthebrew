    <script src="/js/slidey.js"></script>
    <script src="/js/notSlidey.js"></script>
    <script src="/js/scripts.js"></script>
    <script id="beer-template"type="text/x-handlebars-template">
      <div class="a-beer-container">
        <div class="beer-name">{{name}}</div>
        <div class="brewery">Brewery:&nbsp{{brewery}}</div>
        <div class="abv">ABV:&nbsp{{nutritional_value.abv}}%</div>
        <div class="ibu">IBU:&nbsp{{nutritional_value.ibu}}</div>
        <div class="calories">Calories per pint:&nbsp{{nutritional_value.calories}}</div>
        <div id="{{tasteMapId}}"></div>
        <button type="button" class="info" search="{{name}}">get more info!</button>
      </div>
    </script>
   <script id="moreInfo-template"type="text/x-handlebars-template">
      <h1>{{name}}<h1>
      <img src="{{beer_label}}"/>
      <div>ABV:&nbsp{{abv}}</div>
      <div>{{beer_style}}</div>
      <div>{{beer_description}}</div>
      <div id="{{mapId}}"></div>
    </script>

